{
  "name": "lawn-mower",
  "version": "1.0.0",
  "description": "A lawn mower with Babel, React & Flux",
  "main": "src/index.js",
  "scripts": {
    "build": "npm run clean && ./node_modules/.bin/babel-node ./node_modules/.bin/gulp build",
    "clean": "./node_modules/.bin/babel-node ./node_modules/.bin/gulp clean",
    "lint": "./node_modules/.bin/babel-node ./node_modules/.bin/gulp lint",
    "start": "npm run build && ./node_modules/.bin/babel-node ./server/index.server.js",
    "server": "./node_modules/.bin/babel-node ./server/index.server.js",
    "test": "./node_modules/.bin/babel-node ./node_modules/.bin/gulp test",
    "validate": "npm run lint && npm run test",
    "watch": "./node_modules/.bin/babel-node ./node_modules/.bin/gulp watch"
  },
  "repository": "git@github.com:sebastiendavid/lawn-mower.git",
  "author": "SÃ©bastien David <sebastiendavid83@icloud.com>",
  "license": "ISC",
  "homepage": "https://github.com/sebastiendavid/lawn-mower",
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.24",
    "babel-eslint": "^4.1.3",
    "babelify": "^6.3.0",
    "browserify": "^11.1.0",
    "browserify-shim": "^3.8.10",
    "del": "^2.0.2",
    "eslint": "^1.4.3",
    "eslint-config-airbnb": "0.0.8",
    "eslint-plugin-react": "^3.4.1",
    "express": "^4.13.3",
    "flux": "^2.1.1",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^3.0.1",
    "gulp-eslint": "^1.0.0",
    "gulp-if": "^1.2.5",
    "gulp-istanbul": "^0.10.0",
    "gulp-less": "^3.0.3",
    "gulp-minify-css": "^1.2.1",
    "gulp-minify-html": "^1.0.4",
    "gulp-mocha": "^2.1.3",
    "gulp-rename": "^1.2.2",
    "gulp-template": "^3.0.0",
    "gulp-uglify": "^1.4.1",
    "isparta": "^3.0.4",
    "morgan": "^1.6.1",
    "normalize.css": "^3.0.3",
    "open": "0.0.5",
    "proxyquire": "^1.7.2",
    "react": "^0.13.3",
    "reactify": "^1.1.1",
    "serve-static": "^1.10.0",
    "should": "^7.1.0",
    "sinon": "^1.16.1",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "browserify": {
    "transform": [
      "babelify",
      "reactify",
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "flux": "global:Flux",
    "react": "global:React"
  }
}
